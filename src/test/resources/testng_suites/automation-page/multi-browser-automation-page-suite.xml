<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="Multi Browser test suite" verbose="1" parallel="tests" thread-count="2">

    <test name="Test chrome">
        <parameter name="browser" value="chrome"/>
        <classes>
            <class name="com.solvd.carina.automationwebpage.WebMultipleBrowserTest"/>
        </classes>
    </test>

    <test name="Test Firefox">
        <parameter name="browser" value="firefox"/>
        <classes>
            <class name="com.solvd.carina.automationwebpage.WebMultipleBrowserTest"/>
        </classes>
    </test>
</suite>